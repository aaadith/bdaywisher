<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>cron</servlet-name>
		<servlet-class>com.appspot.bdaywisher.cron.CronServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>cron</servlet-name>
		<url-pattern>/cron</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>MainPage</servlet-name>
		<servlet-class>com.appspot.bdaywisher.ui.MainPageServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MainPage</servlet-name>
		<url-pattern>/MainPage</url-pattern>
	</servlet-mapping>
	

	<servlet>
		<servlet-name>AddBDay</servlet-name>
		<servlet-class>com.appspot.bdaywisher.ui.AddBDayServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddBDay</servlet-name>
		<url-pattern>/AddBDay</url-pattern>
	</servlet-mapping>


	
</web-app>
